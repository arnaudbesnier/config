[core]
	excludesfile = /Users/arnaudbesnier/.gitignore_global
	editor = atom --wait
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[alias]
	co = checkout
	ci = commit
	br = branch
	st = status
[user]
	email = arnaudibesnier@gmail.com
	name = Arnaud Besnier
[merge]
	ff = only
